
{
  "weights": {
    "time_weight": 0.5,
    "cost_weight": 0.5
  },
  "constraints": [
    "sum(cost) <= 1800",
    "min(shots) >= 300",
    "min(fidelity) >= 0.9",
    "max(execution_time + waiting_time) <= 2000"
  ],
  "objective": "time_weight * max(execution_time + waiting_time) + cost_weight * sum(cost)"
}